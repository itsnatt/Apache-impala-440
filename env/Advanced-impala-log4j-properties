
main.logger=FA
impala.root.logger=INFO,FA
log4j.rootLogger=${impala.root.logger}
max.log.file.size=200MB
log4j.appender.FA=org.apache.log4j.FileAppender
log4j.appender.FA.layout=org.apache.log4j.PatternLayout
log4j.appender.FA.File=${impala.log.dir}/impalad.INFO
log4j.appender.FA.layout.ConversionPattern=%d{yy/MM/dd HH:mm:ss} %p %c{2}: %m%n
log4j.appender.console=org.apache.log4j.ConsoleAppender
log4j.appender.console.target=System.err
log4j.appender.console.layout=org.apache.log4j.PatternLayout
log4j.appender.console.layout.ConversionPattern=%d{yy/MM/dd HH:mm:ss} %p %c{2}: %m%n
